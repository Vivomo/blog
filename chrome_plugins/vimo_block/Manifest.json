{
  "name" : "Vimo Block",
  "version" : "1.0.0",
  "manifest_version": 2,
  "description": "Disable what I don't want to see",
  "icons": {
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start",
      "matches": [ "http://*/*", "https://*/*" ],
      "js": ["block.js"]
    },{
      "all_frames": true,
      "js": [ "block2.js" ],
      "match_about_blank": true,
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
    }
  ]
}
